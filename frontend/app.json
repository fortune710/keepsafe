{
  "expo": {
    "name": "keepsafe",
    "slug": "keepsafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/keepsafe-logo.png",
    "splash": {
      "image": "./assets/images/keepsafe-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B5CF6"
    },
    "scheme": "keepsafe",
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "associatedDomains": [
        "applinks:keepsafe.fortunealebiosu.dev",
        "applinks:www.keepsafe.fortunealebiosu.dev",
        "applinks:app.keepsafe.fortunealebiosu.dev"
      ],
      "bundleIdentifier": "com.fortunethedev.keepsafe",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "processing"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "97eb5c9e-4f9a-47a1-9994-1351aca19f05"
      },
      "router": {}
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "keepsafe.fortunealebiosu.dev"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_CONTACTS",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WRITE_CONTACTS"
      ],
      "package": "com.fortune710.keepsafe"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/keepsafe-logo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "http://localhost:8081/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/keepsafe-logo.png",
          "color": "#8B5CF6",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Keepsafe to access your camera",
          "microphonePermission": "Allow Keepsafe to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Keepsafe to access your contacts."
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": false
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow Keepsafe to access your microphone."
        }
      ],
      "expo-background-task",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/keepsafe-logo.png",
      "color": "#8B5CF6"
    },
    "owner": "fortune710",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/97eb5c9e-4f9a-47a1-9994-1351aca19f05"
    }
  }
}
